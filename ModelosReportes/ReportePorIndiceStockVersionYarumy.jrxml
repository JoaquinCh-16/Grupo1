	<query language="sql"><![CDATA[SELECT
    PRODUCTO_ID,
    NOMBRE,
    STOCK AS STOCK_DISPONIBLE,
    10 AS STOCK_MINIMO,
    PRECIO, 
    -- El resultado serÃ¡ un decimal entre 0.0 y X.X (ej. 0.2, 1.0, 1.2)
    (CAST(STOCK AS DECIMAL(10, 2)) / 10.0) AS INDICE_STOCK_PORCENTUAL
FROM
    PRODUCTO
WHERE
    ESTADO = 'Activo'
ORDER BY
    INDICE_STOCK_PORCENTUAL ASC]]></query>
