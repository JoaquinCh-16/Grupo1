	<parameter name="FECHA_REPORTE" class="java.sql.Date"/>
	<query language="sql"><![CDATA[SELECT
    P.VENTA_ID AS VENTA_ID,
    P.FECHA_VENTA AS FECHA_VENTA,
    P.COMPRADOR AS COMPRADOR,
    P.TOTAL AS TOTAL_VENTA,
    P.METODO_PAGO AS METODO_PAGO,
    PE.NOMBRE AS NOMBRE_PERSONAL
FROM
    PEDIDO P
JOIN
    PERSONAL PE ON P.PERSONAL_ID = PE.PERSONAL_ID
WHERE
    P.FECHA_VENTA = $P{FECHA_REPORTE} -- Importante: Usa este filtro
ORDER BY
    P.VENTA_ID ASC]]></query>
